# $Id: Makefile.in,v 1.1 2003-03-15 16:13:15 ghudson Exp $

SHELL=/bin/sh
VPATH=@srcdir@
INSTALL=@INSTALL@
INSTALL_PROGRAM=@INSTALL_PROGRAM@
srcdir=@srcdir@
top_srcdir=@top_srcdir@
prefix=@prefix@
exec_prefix=@exec_prefix@
bindir=@bindir@
mandir=@mandir@

CC=@CC@
DEFS=@DEFS@
CPPFLAGS=@CPPFLAGS@
CFLAGS=@CFLAGS@
LDFLAGS=@LDFLAGS@
LIBS=@LIBS@
ALL_CFLAGS=`pkg-config --cflags gtk+-2.0` ${CPPFLAGS} ${CFLAGS} ${DEFS}
ALL_LIBS=`pkg-config --libs gtk+-2.0` ${LIBS}

all: gathlogout

gathlogout: gathlogout.o
	${CC} ${CFLAGS} ${LDFLAGS} -o $@ gathlogout.o ${ALL_LIBS}

.c.o:
	${CC} -c ${ALL_CFLAGS} $<

check:

install:
	${top_srcdir}/mkinstalldirs ${DESTDIR}${bindir}
	${INSTALL_PROGRAM} gathlogout ${DESTDIR}${bindir}

clean:
	rm -f gathlogout.o gathlogout

distclean: clean
	rm -f config.cache config.log config.status Makefile
